{
    "PUSH" :
    {
        "description" : "sp = sp - 4; mem[sp] = R",
        "code" : "0x81E0[R]{-4}"
    },
    "POP" :
    {
        "description" : "R = mem[sp]; sp = sp + 4",
        "code" : "0x93[R]E0{4}"
    },
    "LD" :
    {
        "description" : "R  = operand",
        "types" :
        [
            {
                "description" : "LD r[A], r[B]",
                "code" : "0x91[A][B]0000"
            },
            {
                "description" : "LD mem[A], B",
                "code" : "0x93[B][A]0000"
            },
            {
                "description" : "LD mem[r[A] + offset], r[B]",
                "code" : ["0x91[A][A]0{offset}", "0x93[B][A]0000"]
            },
            {
                "description" : "LD mem[value], r[B]",
                "code_meaning" : "[push r0; r0 = r0 - r0; r0 = r0 + value; r[B] = mem[r0]; pop r0;]",
                "code" : ["0x81E00{-4}", "0x51000000", "0x91000{val}", "0x93[R]00000", "0x930E0{4}"]
            }   
        ]
    },
    "ST" :
    {
        "description" : "operand = R",
        "types" :
        [
            {
                "description" : "ST r[A], r[B]",
                "code" : "0x91[A][B]0000"
            },
            {
                "description" : "ST r[B], mem[r[A]]",
                "code" : "0x81[A][B]0000"
            },
            {
                "description" : "ST r[B], mem[r[A] + offset]",
                "code" : "0x81[A]0[B]{offset}"
            },
            {
                "description" : "ST r[B], mem[value]",
                "code_meaning" : "[push r0; r0 = r0 - r0; r0 = r0 + value; mem[r0] = r[B]; pop r0;]",
                "code" : ["0x81E00{-4}", "0x51000000", "0x91000{val}", "0x810[B]0000", "0x930E0{4}"]
            }   
        ]
    }
